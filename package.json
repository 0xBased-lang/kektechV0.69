{
  "name": "kektech-nextjs",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "generate-metadata": "node scripts/generate-metadata.js",
    "prebuild:full": "npm run generate-metadata",
    "test-rpc": "node scripts/test-rpc-connection.js",
    "check-deployment": "node scripts/pre-deployment-check.js",
    "deploy:offers": "hardhat run scripts/deploy-kektv-offers.js --network basedai",
    "compile:contracts": "hardhat compile",
    "test:contracts": "hardhat test",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "deploy": "bash ./scripts/pre-deploy.sh && vercel --prod --yes",
    "deploy:preview": "bash ./scripts/pre-deploy.sh && vercel --yes"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.80.0",
    "@tanstack/react-query": "^5.90.5",
    "@types/dompurify": "^3.0.5",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csrf": "^3.1.0",
    "date-fns": "^2.30.0",
    "dompurify": "^3.3.0",
    "lucide-react": "^0.548.0",
    "next": "15.5.5",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "siwe": "^3.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.38.4",
    "wagmi": "^2.18.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^4.0.9",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "eslint-config-prettier": "^10.1.8",
    "hardhat": "^2.26.3",
    "jsdom": "^27.0.1",
    "playwright": "^1.56.1",
    "playwright-extra": "^4.3.6",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "puppeteer-core": "^24.29.1",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@tailwindcss/oxide-linux-arm64-gnu": "4.1.16",
    "@tailwindcss/oxide-linux-x64-gnu": "4.1.16",
    "lightningcss-linux-arm64-gnu": "^1.30.2",
    "lightningcss-linux-x64-gnu": "^1.30.2"
  }
}
